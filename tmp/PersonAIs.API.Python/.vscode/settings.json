{
    "customCommands.commands": [
        {
            "name": "🗑️ Delete all __pycache__ files",
            "command": "Get-ChildItem -Path . -Recurse -Directory -Filter '__pycache__' | Remove-Item -Recurse -Force",
            "runOnStartup": true,
            "runOnShutdown": true
        }
    ]
}